@model Sistema_MVC_Mamani.Models.Semestre
@{
    ViewBag.Title = "visualizar";
}

<center><h2 class="card text-center">visualizar</h2></center>
<nav aria-label="breadcrumb">
    <ol class=" breadcrumb">
        <li class="breadcrumb-item">
            <a href="~/semestre"> Semestre</a>
        </li>
        <li class=" breadcrumb-item active">@Model.nombre</li>

    </ol>

</nav>


<div class="panel panel-default">

    <div class="panel-heading">
        Informacion detallada del Semestre
    </div>
    <div class="panel-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.semestre_id</dd>
            <dt>Nombre</dt>
            <dd>@Model.nombre</dd>
            <dt>Año</dt>
            <dd>@Model.anio</dd>
            <dt>Estado</dt>
            <dd>@Model.estado</dd>
        </dl>
    </div>

</div>
<br /><br />