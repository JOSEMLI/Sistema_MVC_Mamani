@model Sistema_MVC_Mamani.Models.Usuario
@{
    ViewBag.Title = "visualizar";
}


<center><h2 class="card text-center">visualizar</h2></center>
<nav aria-label="breadcrumb">
    <ol class=" breadcrumb">
        <li class="breadcrumb-item">
            <a href="~/usuario"> Usuario</a>
        </li>
        <li class=" breadcrumb-item active">@Model.nombre</li>

    </ol>

</nav>


<div class="panel panel-default">

    <div class="panel-heading">
        Informacion detallada del Usuario
    </div>
    <div class="panel-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.usuario_id</dd>
            <dt>Docente</dt>
            <dd>@Model.Docente.apellido , @Model.Docente.nombre</dd>
            <dt>Nombre</dt>
            <dd>@Model.nombre</dd>
            <dt>Nivel</dt>
            <dd>@Model.nivel</dd>
            <dt>Avatar</dt>
            <dd>@Model.avatar</dd>
            <dt>Estado</dt>
            <dd>@Model.estado</dd>
        </dl>
    </div>

</div>
<br /><br />

