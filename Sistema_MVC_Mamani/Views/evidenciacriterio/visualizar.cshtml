@model Sistema_MVC_Mamani.Models.EvidenciaCriterio
@{
    ViewBag.Title = "visualizar";
}
<center><h2 class="card text-center">visualizar</h2></center>
<nav aria-label="breadcrumb">
    <ol class=" breadcrumb">
        <li class="breadcrumb-item">
            <a href="~/evidenciaactividad"> Evidencia Criterio</a>
        </li>
        <li class=" breadcrumb-item active">@Model.Criterio.nombre_spanish</li>

    </ol>

</nav>
<br />
<div class="panel panel-default">

    <div class="panel-heading">
        Informacion detallada de la Evidencia Criterio
    </div>
    <div class="panel-body">
        <dl class="dl-horizontal">
            <dt>Id</dt>
            <dd>@Model.evidencia_id</dd>
            <dt>Criterio</dt>
            <dd>@Model.Criterio.nombre_spanish</dd>
            <dt>Archivo</dt>
            <dd>@Model.archivo</dd>
            <dt>Tamaño</dt>
            <dd>@Model.tamanio</dd>
            <dt>Tipo</dt>
            <dd>@Model.tipo</dd>
            <dt>Descripcion</dt>
            <dd>@Model.descripcion</dd>
        </dl>
    </div>

</div>
<br />

